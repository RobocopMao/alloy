* {
  margin 0
  padding 0
  font-weight normal
  font-family 'Microsoft YaHei', 'Avenir', Helvetica, Arial, sans-serif
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  color #FA6775
}
a {
  text-decoration none
}
ul, li {
  list-style-type none
}
.page {
  position relative
  height 100%
}
